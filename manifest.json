{
  "manifestVersion": 4,
  "id": "123456",
  "name": "AdobeXD Alert Plugin",
  "version": "0.0.1",
  "host": {
    "app": "XD",
    "minVersion": "36.0"
  },
  "entryPoints": [
    {
      "type": "command",
      "id": "showAlert",
      "label": {
        "default": "How to show an alert"
      },
      "shortcut": {
        "mac": "Cmd+Alt+A",
        "windows": "CtrlAlt+A"
      }
    },
    {
      "type": "command",
      "id": "showError",
      "label": {
        "default": "How to show an error"
      },
      "shortcut": {
        "mac": "Cmd+Alt+E",
        "windows": "CtrlAlt+E"
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "icons/icon.png",
      "scale": [1,2],
      "theme": ["all"],
      "species": ["pluginList"]
    }
  ]
}